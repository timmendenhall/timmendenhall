{
  "name": "timmendenhall",
  "private": true,
  "version": "0.0.1",
  "description": "Tim Mendenhall Portfolio SPA",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.prod.html ./src/favicon.ico ./dist",
    "build": "npm run clean && npm run copy && NODE_ENV=production webpack --env=dist",
    "dev": "node server.js --env=dev",
    "prod-start": "sudo PORT=80 NODE_ENV=production forever start server.js",
    "prod-stop": "sudo forever stop 0",
    "prod-restart": "sudo forever restart 0",
    "prod-test": "NODE_ENV=production node server.js",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "test": "NODE_ENV=test karma start",
    "test:watch": "NODE_ENV=test karma start --autoWatch=true --singleRun=false"
  },
  "repository": "",
  "keywords": [],
  "author": "Tim Mendenhall",
